<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FilterCalenderView">
        <attr name="arrowBackgroundColor" format="color" />
        <attr name="arrowIconColor" format="color" />
        <attr name="optionSelectedBackgroundColor" format="color" />
        <attr name="optionUnselectedBackgroundColor" format="color" />
        <attr name="optionSelectedStrokeColor" format="color" />
        <attr name="optionUnselectedStrokeColor" format="color" />
        <attr name="optionSelectedTextColor" format="color" />
        <attr name="optionUnselectedTextColor" format="color" />
        <attr name="displayTextColor" format="color" />
        <attr name="optionTextSize" format="dimension" />
        <attr name="displayTextSize" format="dimension" />
        <attr name="defaultSelectedOption" format="enum">
            <enum name="None" value="0" />
            <enum name="All" value="1" />
            <enum name="Daily" value="2" />
            <enum name="Weekly" value="3" />
            <enum name="Monthly" value="4" />
            <enum name="Yearly" value="5" />
        </attr>
    </declare-styleable>
</resources>